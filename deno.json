{
  "name": "@canac/bundle-blame",
  "version": "0.1.0",
  "license": "MIT",
  "exports": "./main.ts",
  "tasks": {
    "dev": "deno run --allow-env=HOME --allow-read --allow-write --allow-run --watch main.ts"
  },
  "imports": {
    "@david/dax": "jsr:@david/dax@^0.43",
    "@std/assert": "jsr:@std/assert@^1",
    "@std/collections": "jsr:@std/collections@^1",
    "@std/fmt": "jsr:@std/fmt@^1",
    "@std/fs": "jsr:@std/fs@^1",
    "@std/path": "jsr:@std/path@^1"
  },
  "fmt": {
    "lineWidth": 100
  },
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "*.ts"
    ]
  },
  "compilerOptions": {
    "noUncheckedIndexedAccess": true
  }
}
